<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.android.baoshantourguide">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".ParkActivity"
            android:label="Parks">
        </activity>
        <activity android:name=".ShoppingmallActivity"
            android:label="Shopping malls">
        </activity>
        <activity android:name=".RestaurantActivity"
            android:label="Restaurants">
        </activity>
        <activity android:name=".UniversityActivity"
            android:label="Universities">
        </activity>
    </application>

</manifest>